{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "cdw": ".."
        }
      ],
      "dest": "docfx/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docfx/api/**.yml",
          "docfx/api/index.md"
        ]
      },
      {
        "files": [
          "docfx/articles/**.md",
          "docfx/articles/**/toc.yml",
          "docfx/toc.yml",
          "docfx/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docfx/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docfx/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../doc/html",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
